{
	"activationEvents": [
		"workspaceContains:sfdx-project.json",
		"onCommand:apexrunner.executeAnonymous"
	],
	"author": "Joseph Allen",
	"bugs": {
		"url": "https://github.com/JosephAllen/apex-code-runner/issues"
	},
	"categories": [
		"Debuggers",
		"Other",
		"Programming Languages"
	],
	"contributes": {
		"commands": [
			{
				"command": "apexrunner.executeAnonymous",
				"title": "Apex: Execute Anonymous",
				"when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
			}
		],
		"configuration": {
			"properties": {
				"apex-code-runner.logCategoryOptions": {
					"default": {
						"Apex_code": "Debug",
						"Apex_profiling": "Info",
						"Callout": "Info",
						"Db": "Info",
						"System": "Debug",
						"Validation": "Info",
						"VisualForce": "Info",
						"Workflow": "Info"
					},
					"description": "Apex log categories with their associated log levels. These only affect the level of detail in the full log output window.",
					"properties": {
						"Apex_code": {
							"default": "Debug",
							"enum": [
								"Error",
								"Warn",
								"Info",
								"Debug",
								"Fine",
								"Finer",
								"Finest"
							],
							"type": "string"
						},
						"Apex_profiling": {
							"default": "Info",
							"enum": [
								"None",
								"Error",
								"Warn",
								"Info",
								"Debug",
								"Fine",
								"Finer",
								"Finest"
							],
							"type": "string"
						},
						"Callout": {
							"default": "Info",
							"enum": [
								"None",
								"Error",
								"Warn",
								"Info",
								"Debug",
								"Fine",
								"Finer",
								"Finest"
							],
							"type": "string"
						},
						"Db": {
							"default": "Info",
							"enum": [
								"None",
								"Error",
								"Warn",
								"Info",
								"Debug",
								"Fine",
								"Finer",
								"Finest"
							],
							"type": "string"
						},
						"System": {
							"default": "Debug",
							"enum": [
								"None",
								"Error",
								"Warn",
								"Info",
								"Debug",
								"Fine",
								"Finer",
								"Finest"
							],
							"type": "string"
						},
						"Validation": {
							"default": "Info",
							"enum": [
								"None",
								"Error",
								"Warn",
								"Info",
								"Debug",
								"Fine",
								"Finer",
								"Finest"
							],
							"type": "string"
						},
						"VisualForce": {
							"default": "Info",
							"enum": [
								"None",
								"Error",
								"Warn",
								"Info",
								"Debug",
								"Fine",
								"Finer",
								"Finest"
							],
							"type": "string"
						},
						"Workflow": {
							"default": "Info",
							"enum": [
								"None",
								"Error",
								"Warn",
								"Info",
								"Debug",
								"Fine",
								"Finer",
								"Finest"
							],
							"type": "string"
						}
					},
					"required": [
						"Apex_code"
					],
					"type": "object"
				},
				"apex-code-runner.preferredWindow": {
					"default": "userDebug",
					"description": "Output window to display when execution completes. You can view either at any time.",
					"enum": [
						"userDebug",
						"fullLog"
					],
					"enumDescriptions": [
						"Shows only system.debug statements (and errors)",
						"Shows the Full log will show the full log"
					],
					"type": "string"
				}
			},
			"title": "Apex Code Runner",
			"type": "object"
		},
		"keybindings": [],
		"languages": [
			{
				"aliases": [
					"Apex"
				],
				"extensions": [
					".apex",
					".soql"
				],
				"id": "apex"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "apexrunner.executeAnonymous",
					"group": "ApexRunner",
					"when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
				}
			],
			"editor/context": [
				{
					"command": "apexrunner.executeAnonymous",
					"group": "ApexRunner",
					"when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
				}
			],
			"editor/title": [
				{
					"command": "apexrunner.executeAnonymous",
					"group": "ApexRunner",
					"when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
				}
			]
		}
	},
	"contributors": [
		"Joseph Allen (https://github.com/JosephAllen)"
	],
	"dependencies": {
		"@salesforce/core": "^2.4.0",
		"@types/node-fetch": "^2.5.7",
		"node-fetch": "^2.6.0",
		"xml2js": "^0.4.23"
	},
	"description": "Execute Apex scripts in a Salesforce project",
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@types/xml2js": "^0.4.5",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"minimist": "^1.2.5",
		"mkdirp": "^1.0.4",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"displayName": "Apex Code Runner",
	"engines": {
		"vscode": "^1.44.0"
	},
	"homepage": "https://github.com/JosephAllen/apex-code-runne",
	"icon": "images/icon.png",
	"keywords": [
		"anonymous apex",
		"apex",
		"apex-anon",
		"console.log",
		"debug",
		"salesforce dx",
		"salesforce",
		"salesforcedx",
		"sfdx",
		"soql",
		"system.debug",
		"user debug"
	],
	"license": "MIT",
	"main": "./out/extension",
	"name": "apex-code-runner",
	"publishConfig": {
		"access": "public"
	},
	"publisher": "JosephAllen",
	"repository": {
		"type": "git",
		"url": "https://github.com/JosephAllen/apex-code-runner"
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"version": "0.0.4"
}
