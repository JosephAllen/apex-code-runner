{
	"activationEvents": [
		"workspaceContains:sfdx-project.json",
		"onCommand:apexrunner.executeAnonymous"
	],
	"author": "Joseph Allen",
	"bugs": {
		"url": "https://someurl"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"commands": [
			{
				"command": "apexrunner.executeAnonymous",
				"title": "Apex: Execute Anonymous",
				"when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
			}
		],
		"keybindings": [],
		"languages": [
			{
				"aliases": [
					"Apex"
				],
				"extensions": [
					".apex",
					".soql"
				],
				"id": "apex"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "apexrunner.executeAnonymous",
					"group": "ApexRunner",
					"when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
				}
			],
			"editor/context": [
				{
					"command": "apexrunner.executeAnonymous",
					"group": "ApexRunner",
					"when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
				}
			],
			"editor/title": [
				{
					"command": "apexrunner.executeAnonymous",
					"group": "ApexRunner",
					"when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
				}
			]
		}
	},
	"contributors": [
		"Joseph Allen (https://github.com/JosephAllen)"
	],
	"dependencies": {
		"@salesforce/core": "^2.3.1",
		"@types/node-fetch": "^2.5.6",
		"node": "^14.0.0",
		"npm": "^6.14.4"
	},
	"description": "Executes Apex code and quickly returns a formatted User Debug",
	"devDependencies": {
		"@salesforce/core": "^2.3.1",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@types/xml2js": "^0.4.5",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"node-fetch": "^2.6.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"xml2js": "^0.4.23"
	},
	"displayName": "Apex Code Runner",
	"engines": {
		"vscode": "^1.44.0"
	},
	"homepage": "https://github.com/JosephAllen/apex-code-runne",
	"icon": "images/icon.png",
	"keywords": [
		"apex",
		"debug",
		"force",
		"salesforce",
		"salesforcedx",
		"sfdx"
	],
	"license": "MIT",
	"main": "./out/extension",
	"name": "apex-code-runner",
	"publishConfig": {
		"access": "public"
	},
	"publisher": "JosephAllen",
	"repository": {
		"type": "git",
		"url": "https://github.com/JosephAllen/apex-code-runner"
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"version": "0.0.1"
}