{
  "activationEvents": [
    "workspaceContains:sfdx-project.json",
    "onCommand:apexrunner.executeAnonymous"
  ],
  "author": "Joseph Allen",
  "bugs": {
    "url": "https://github.com/JosephAllen/apex-code-runner/issues"
  },
  "categories": [
    "Debuggers",
    "Other",
    "Programming Languages"
  ],
  "contributes": {
    "commands": [
      {
        "command": "apexrunner.executeAnonymous",
        "title": "Apex: Execute Anonymous",
        "when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
      },
      {
        "command": "apexrunner.refreshToken",
        "title": "Apex: Refresh Access Token"
      }
    ],
    "configuration": {
      "properties": {
        "apex-code-runner.logCategoryOptions": {
          "default": {
            "Apex_code": "Debug",
            "Apex_profiling": "Info",
            "Callout": "Info",
            "Db": "Info",
            "System": "Debug",
            "Validation": "Info",
            "VisualForce": "Info",
            "Workflow": "Info"
          },
          "description": "Apex log categories with their associated log levels. These only affect the level of detail in the full log output window.",
          "properties": {
            "Apex_code": {
              "default": "Debug",
              "enum": [
                "Error",
                "Warn",
                "Info",
                "Debug",
                "Fine",
                "Finer",
                "Finest"
              ],
              "type": "string"
            },
            "Apex_profiling": {
              "default": "Info",
              "enum": [
                "None",
                "Error",
                "Warn",
                "Info",
                "Debug",
                "Fine",
                "Finer",
                "Finest"
              ],
              "type": "string"
            },
            "Callout": {
              "default": "Info",
              "enum": [
                "None",
                "Error",
                "Warn",
                "Info",
                "Debug",
                "Fine",
                "Finer",
                "Finest"
              ],
              "type": "string"
            },
            "Db": {
              "default": "Info",
              "enum": [
                "None",
                "Error",
                "Warn",
                "Info",
                "Debug",
                "Fine",
                "Finer",
                "Finest"
              ],
              "type": "string"
            },
            "System": {
              "default": "Debug",
              "enum": [
                "None",
                "Error",
                "Warn",
                "Info",
                "Debug",
                "Fine",
                "Finer",
                "Finest"
              ],
              "type": "string"
            },
            "Validation": {
              "default": "Info",
              "enum": [
                "None",
                "Error",
                "Warn",
                "Info",
                "Debug",
                "Fine",
                "Finer",
                "Finest"
              ],
              "type": "string"
            },
            "VisualForce": {
              "default": "Info",
              "enum": [
                "None",
                "Error",
                "Warn",
                "Info",
                "Debug",
                "Fine",
                "Finer",
                "Finest"
              ],
              "type": "string"
            },
            "Workflow": {
              "default": "Info",
              "enum": [
                "None",
                "Error",
                "Warn",
                "Info",
                "Debug",
                "Fine",
                "Finer",
                "Finest"
              ],
              "type": "string"
            }
          },
          "required": [
            "Apex_code"
          ],
          "type": "object"
        },
        "apex-code-runner.preferredWindow": {
          "default": "userDebug",
          "description": "Output window to display when execution completes. You can view either at any time.",
          "enum": [
            "userDebug",
            "fullLog"
          ],
          "enumDescriptions": [
            "Shows only system.debug statements (and errors)",
            "Shows the Full log will show the full log"
          ],
          "type": "string"
        }
      },
      "title": "Salesforce Apex Code Runner",
      "type": "object"
    },
    "keybindings": [],
    "languages": [
      {
        "aliases": [
          "Apex"
        ],
        "extensions": [
          ".apex",
          ".soql"
        ],
        "id": "apex"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "apexrunner.executeAnonymous",
          "group": "ApexRunner",
          "when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
        },
        {
          "command": "apexrunner.refreshToken",
          "group": "ApexRunner"
        }
      ],
      "editor/context": [
        {
          "command": "apexrunner.executeAnonymous",
          "group": "ApexRunner",
          "when": "APXRActive && resourceExtname =~ /(.apex|.soql)/"
        },
        {
          "command": "apexrunner.refreshToken",
          "group": "ApexRunner",
          "when": "APXRActive == false"
        }
      ]
    }
  },
  "contributors": [
    "Joseph Allen (https://github.com/JosephAllen)"
  ],
  "dependencies": {
    "node-fetch": "^3.1.1",
    "xml2js": "^0.4.23"
  },
  "description": "Execute Apex scripts in a Salesforce project",
  "devDependencies": {
    "@types/node": "^13.13.52",
    "@types/node-fetch": "^2.5.12",
    "@types/vscode": "^1.44.1",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "glob-parent": "^6.0.2",
    "serialize-javascript": "^6.0.0",
    "set-value": ">=4.0.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.10",
    "vscode-test": "^1.6.1",
    "webpack": "^5.67.0",
    "webpack-cli": "^3.3.12"
  },
  "displayName": "Apex Code Runner",
  "engines": {
    "vscode": "^1.44.1"
  },
  "extensionDependencies": [
    "salesforce.salesforcedx-vscode-core"
  ],
  "homepage": "https://github.com/JosephAllen/apex-code-runne",
  "icon": "images/icon.png",
  "keywords": [
    "anonymous apex",
    "apex",
    "apex-anon",
    "console.log",
    "debug",
    "salesforce dx",
    "salesforce",
    "salesforcedx",
    "sfdx",
    "soql",
    "system.debug",
    "user debug"
  ],
  "license": "MIT",
  "main": "./dist/extension",
  "name": "apex-code-runner",
  "publishConfig": {
    "access": "public"
  },
  "publisher": "JosephAllen",
  "repository": {
    "type": "git",
    "url": "https://github.com/JosephAllen/apex-code-runner"
  },
  "scripts": {
    "compile": "webpack --mode none",
    "package": "rimraf ./dist && rimraf ./*.vsix && webpack --mode production && vsce package",
    "test": "node ./dist/test/runTest.js",
    "test-compile": "tsc -p ./",
    "vscode:prepublish": "webpack --mode production",
    "watch": "webpack --mode none --watch",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch"
  },
  "version": "0.1.4"
}
